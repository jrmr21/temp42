#include <stdio.h>

int ft_sqrt(int nb);

int ft_sqrt(int nb)
{
	int i;

	i = 1;
	while (i*i < nb)
		i++;

	return (i);
}

int	main(void)
{
	printf("%d", ft_sqrt(9));
}
