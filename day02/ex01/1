#include <unistd.h>

void ft_print_reverse_alphabet(void);

int		main(void)
{
	ft_print_reverse_alphabet();
	return 0;
}

void ft_print_reverse_alphabet(void)
{
	char *fr;
	fr = "zyxwvutsrqponmlkjihgfedcbda";
	/*while(*fr)
	{
		ft_putchar(*fr++);
	}*/

	write(1,*fr,*fr);
}
